{
    "name": "folklore/laravel-mediatheque",
    "description": "",
    "keywords": [
        "laravel"
    ],
    "homepage": "http://github.com/Folkloreatelier",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@atelierfolklore.ca",
            "homepage": "http://atelierfolklore.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "php-ffmpeg/php-ffmpeg": "^0.17.0",
        "symfony/http-foundation": "^5.0",
        "symfony/process": "^5.0",
        "ramsey/uuid": "^3.7|^4.0",
        "symfony/mime": "^5.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "orchestra/database": "^5.0|^6.0",
        "orchestra/testbench": "^5.0|^6.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "doctrine/dbal": "^2.6",
        "php-coveralls/php-coveralls": "^2.4",
        "league/color-extractor": "0.3.*"
    },
    "suggests": {
        "ext-imagick": "*",
        "ext-gd": "*",
        "league/color-extractor": "0.3.*"
    },
    "autoload": {
        "psr-0": {
            "Folklore\\Mediatheque": "src/"
        },
        "files": [
            "src/Folklore/Mediatheque/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Folklore\\Mediatheque\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Folklore\\Mediatheque\\ServiceProvider"
            ],
            "aliases": {
                "Mediatheque": "Folklore\\Mediatheque\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
